// Test: Nested loops
fn main() {
    let mut i = 0;
    while i < 3 {
        let mut j = 0;
        while j < 3 {
            j = j + 1;
        }
        i = i + 1;
    }
    
    loop {
        let mut k = 0;
        loop {
            k = k + 1;
            if k >= 2 {
                break;
            }
        }
        break;
    }
}
