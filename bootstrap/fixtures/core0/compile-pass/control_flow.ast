// Test fixture: control_flow.ast
// Category: compile-pass
// Tests: if/while/for control flow (Core-0)

fn main() {
    // If-else
    let x = 10;
    if x > 5 {
        print("x is greater than 5");
    } else {
        print("x is not greater than 5");
    }
    
    // While loop
    let mut count = 0;
    while count < 3 {
        print(count);
        count = count + 1;
    }
    
    // For loop (over range)
    for i in 0..5 {
        print(i);
    }
}
