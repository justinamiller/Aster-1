struct TraitContext {
    trait_count: i32,
    impl_count: i32,
    obligation_count: i32
}

fn new_trait_context() -> TraitContext {
    TraitContext {
        trait_count: 0,
        impl_count: 0,
        obligation_count: 0
    }
}

fn add_trait(ctx: TraitContext, name: String) -> TraitContext {
    TraitContext {
        trait_count: ctx.trait_count + 1,
        impl_count: ctx.impl_count,
        obligation_count: ctx.obligation_count
    }
}

